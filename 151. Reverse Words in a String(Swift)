class Solution {
    func reverseWords(_ s: String) -> String {
        let words = s.split(separator: " ").map { String($0) }
        var rev = ""
        for i in stride(from: words.count - 1, through: 0, by: -1){
            rev += words[i]
            if i > 0{
                rev += " "
            }
        }
        return rev
    }
}
